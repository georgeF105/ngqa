{
  "compilerOptions": {
    "declaration": true,
    "stripInternal": true,
    "lib": ["es6", "esnext"],
    "module": "commonjs",
    "noImplicitReturns": true,
    "outDir": "lib",
    "sourceMap": true,
    "target": "es6"
  },
  "compileOnSave": true,
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
